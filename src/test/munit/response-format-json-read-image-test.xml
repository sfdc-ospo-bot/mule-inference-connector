	<munit:test doc:id="b3144fa3-60d9-41f1-9c26-ab5eae501685" name="read_image_url_operation_response_format_json_test">
		<munit:execution>
			<try doc:name="Try">
				<flow-ref doc:id="a79786d9-29f4-4966-996f-fef6f2abcfc7" doc:name="Flow-ref to chat_completion_operation" name="read_image_url_operation"/>
			<error-handler>
				<on-error-continue type="ms-inference:RATE_LIMIT_EXCEEDED" doc:name="On Error Continue with mock values">
					<logger level="INFO" message="Continuing with the flow as rate limit exceeded"/>
					<ee:transform doc:name="Transform Message" doc:id="510a60ca-08b6-4311-b979-cb5f67e8ce4d" >
						<ee:message >
							<ee:set-payload ><![CDATA[%dw 2.0
									output application/json
									---
									{
										"payload": {
											"response":  "{\n  \"description\": {\n    \"landmark\": \"Statue of Liberty\",\n    \"visible_features\": [\n      \"Statue of Liberty\",\n      \"Skyline of Manhattan\"\n    ]\n  }\n}"

										}
									}]]>
							</ee:set-payload>
						</ee:message>
					</ee:transform>
				</on-error-continue>
			</error-handler>
		</try>
		</munit:execution>
		<munit:validation>
			<logger level="DEBUG" message="#[payload]"/>
			<munit-tools:assert-that doc:id="e45fe6bd-e51a-40e6-9c18-e1cc92295832" doc:name="payload.response" expression="#[contains(payload.payload.response, 'Statue of Liberty')]" is="#[MunitTools::equalTo(true)]" message="Payload response is wrong answer"/>
			<!-- Check if response can be parsed as JSON -->
			<munit-tools:assert-that doc:name="response is valid JSON" doc:id="b2c3d4e5-f6g7-8901-bcde-f12345678901" message="Response cannot be parsed as valid JSON"
									 expression="#[%dw 2.0
									 import * from dw::Runtime
									 var parsedResponse = try(() -> read(payload.payload.response, 'application/json'))
									 ---
									 parsedResponse.success]"
									 is="#[MunitTools::equalTo(true)]" />
		</munit:validation>
	</munit:test>

	<sub-flow name="read_image_url_operation" doc:id="df9ead27-c4c2-4b35-87d7-0437bb2316d2">
		<set-variable value='#[%dw 2.0
		   output application/json
		    ---
			{
				"prompt":"What do you see. Please respond in a json format only",
				"imageUrl":"https://cdn.britannica.com/61/93061-050-99147DCE/Statue-of-Liberty-Island-New-York-Bay.jpg",
			}]' doc:name="Set Variable" doc:id="00231872-9564-4fad-b580-4f745c16ac9d" variableName="testPayload"/>
		<ms-inference:read-image doc:name="[Image] Read by (Url or Base64)" doc:id="f2d53937-53d0-4964-b3e5-272c779ffd84" config-ref="${config}">
			<ms-inference:prompt ><![CDATA[#[vars.testPayload.prompt]]]></ms-inference:prompt>
			<ms-inference:image-url ><![CDATA[#[vars.testPayload.imageUrl]]]></ms-inference:image-url>
			<ms-inference:additional-request-attributes ><![CDATA[{
			  "response_format": {
				"type": "json_object"
			  }
			}]]>
		</ms-inference:additional-request-attributes>
		</ms-inference:read-image>
		<ee:transform doc:id="02c49472-abaf-4e69-8a23-431c8b698b07" doc:name="Transform Message">
			<ee:message>
				<ee:set-payload><![CDATA[%dw 2.0
					output application/json
					---
					{
						payload: payload,
						attributes: attributes
					}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</sub-flow>
</mule>